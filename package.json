{
  "name": "scope-server",
  "devDependencies": {
    "@types/clone-deep": "^4.0.1",
    "@types/ejs": "^2.6.3",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.12",
    "@types/nodemailer": "^6.2.0",
    "@types/parse": "^2.2.7",
    "@types/request": "^2.48.1",
    "@types/stripe": "^6.31.22",
    "@types/xml2js": "^0.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-flags": "^0.1.9",
    "node-ssh": "^5.1.2",
    "scss-compile": "^0.1.7",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.3"
  },
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=development parse-server config/parse-server.config.js",
    "app": "cross-env NODE_ENV=development node build/cloud/app",
    "build": "tsc --sourcemap",
    "build:prod": "tsc",
    "clean": "rm -r -f build/cloud",
    "cloneRemoteDatabase": "node scripts/clone-remote-database",
    "dashboard": "parse-dashboard --config config/parse-dashboard.config.json",
    "debug": "cross-env NODE_ENV=development node --inspect node_modules/parse-server/bin/parse-server config/parse-server.config.js",
    "deploy": "tsc && node scripts/deploy --b4a",
    "lint": "tslint src/**/*.ts --project tsconfig.json",
    "mongo": "mongod",
    "prebuild": "npm run clean",
    "predeploy": "npm run clean",
    "postdeploy": "npm run build",
    "sass": "npx node-sass assets/templates/scss -o assets/templates/css",
    "server": "cross-env NODE_ENV=development npx parse-server config/parse-server.config.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "arr-filter-unique": "^1.1.2",
    "array-average": "^0.1.2",
    "array-filter-unique": "^2.2.0",
    "assign-deep": "^1.0.1",
    "clone-deep": "^4.0.1",
    "deep-diff": "^1.0.2",
    "ejs": "^2.6.2",
    "googleapis": "^41.0.0",
    "group-array": "^0.3.3",
    "i18n-iso-countries": "^4.0.0",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "moment-business-days": "^1.1.3",
    "nodemailer": "^6.2.1",
    "parse-server-mailgun-adapter-template": "^1.1.7",
    "stripchar": "^1.2.1",
    "stripe": "^7.4.0",
    "styliner": "^1.0.4",
    "snyk": "^1.222.1"
  },
  "_moduleAliases": {
    "@app/env": "build/cloud/env/env",
    "@library": "build/cloud/library",
    "@modules": "build/cloud/modules",
    "@parse": "build/cloud/library/parse.server"
  },
  "snyk": true
}
