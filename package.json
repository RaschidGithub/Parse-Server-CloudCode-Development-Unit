{
	"name": "parse-server-backend-template",
	"devDependencies": {
		"@types/clone-deep": "^4.0.1",
		"@types/ejs": "^2.6.3",
		"@types/express": "^4.17.0",
		"@types/node": "^12.0.12",
		"@types/nodemailer": "^6.2.0",
		"@types/parse": "^2.2.7",
		"@types/request": "^2.48.1",
		"@types/stripe": "^6.31.22",
		"@types/xml2js": "^0.4.3",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.3.1",
		"cross-env": "^5.2.0",
		"mkdirp": "^0.5.1",
		"ncp": "^2.0.0",
		"node-flags": "^0.1.9",
		"node-ssh": "^5.1.2",
		"scss-compile": "^0.1.7",
		"tslint": "^5.16.0",
		"tslint-eslint-rules": "^5.4.0",
		"typescript": "^3.0.3"
	},
	"scripts": {
		"start": "npm run build && cross-env NODE_ENV=development parse-server config/parse-server.config.js",
		"app": "cross-env NODE_ENV=development node build/cloud/app",
		"build": "tsc --sourcemap",
		"build:prod": "tsc",
		"build:scss": "npx node-sass assets/templates/scss -o assets/templates/css",
		"clean": "rm -r -f build/cloud",
		"cloneRemoteDatabase": "node scripts/clone-remote-database",
		"dashboard": "parse-dashboard --config config/parse-dashboard.config.json",
		"debug": "cross-env NODE_ENV=development node --inspect node_modules/parse-server/bin/parse-server config/parse-server.config.js",
		"deploy": "tsc && node scripts/deploy --b4a",
		"lint": "tslint src/**/*.ts --project tsconfig.json",
		"mongo": "mongod",
		"prebuild": "npm run clean",
		"predeploy": "npm run clean",
		"postdeploy": "npm run build",
		"server": "cross-env NODE_ENV=development npx parse-server config/parse-server.config.js"
	},
	"dependencies": {
		"ejs": "^2.6.2",
		"i18n-iso-countries": "^4.0.0",
		"module-alias": "^2.2.0",
		"nodemailer": "^6.2.1",
		"parse-server-mailgun-adapter-template": "^1.1.7",
		"stripchar": "^1.2.1",
		"stripe": "^7.4.0",
		"styliner": "^1.0.4"
	},
	"_moduleAliases": {
		"@app/env": "build/cloud/env/env",
		"@lib": "build/cloud/lib",
		"@modules": "build/cloud/modules",
		"@parse": "build/cloud/parse.server"
	}
}
