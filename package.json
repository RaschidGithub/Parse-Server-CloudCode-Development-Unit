{
	"name": "scope-server",
	"devDependencies": {
		"@types/ejs": "^2.6.3",
		"@types/express": "^4.17.0",
		"@types/node": "^12.0.12",
		"@types/nodemailer": "^6.2.0",
		"@types/parse": "^2.2.7",
		"@types/request": "^2.48.1",
		"@types/stripe": "^6.31.9",
		"@types/xml2js": "^0.4.3",
		"cross-env": "^5.2.0",
		"mkdirp": "^0.5.1",
		"ncp": "^2.0.0",
		"node-flags": "^0.1.9",
		"node-ssh": "^5.1.2",
		"tslint": "^5.16.0",
		"tslint-eslint-rules": "^5.4.0",
		"typescript": "^3.0.3"
	},
	"scripts": {
		"start": "npm run build && cross-env NODE_ENV=development parse-server config/parse-server.config.js",
		"build": "tsc --sourcemap",
		"build:prod": "tsc",
		"clean": "rm -r --force build/cloud",
		"cloneRemoteDatabase": "node scripts/clone-remote-database",
		"dashboard": "parse-dashboard --config config/parse-dashboard.config.json",
		"debug": "cross-env NODE_ENV=development node --inspect node_modules/parse-server/bin/parse-server config/parse-server.config.js",
		"deploy": "tsc && node scripts/deploy --b4a",
		"lint": "tslint src/**/*",
		"mongo": "mongod",
		"prebuild": "npm run clean",
		"predeploy": "npm run clean",
		"postdeploy": "npm run build",
		"server": "npx parse-server config/parse-server.config.js"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"ejs": "^2.6.2",
		"i18n-iso-countries": "^4.0.0",
		"module-alias": "^2.2.0",
		"stripchar": "^1.2.1"
	},
	"_moduleAliases": {
		"@app/env": "build/cloud/env/env",
		"@library": "build/cloud/library",
		"@modules": "build/cloud/modules",
		"@parse": "build/cloud/library/parse.server"
	}
}