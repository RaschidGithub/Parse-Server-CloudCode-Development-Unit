{
  "name": "ya-server",
  "devDependencies": {
    "@types/clone-deep": "^4.0.1",
    "@types/ejs": "^2.6.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.4.2",
    "@types/node": "^12.0.12",
    "@types/node-gcm": "^1.0.0",
    "@types/nodemailer": "^6.2.0",
    "@types/parse": "^2.12.0",
    "@types/request-promise-native": "^1.0.17",
    "@types/stripe": "^6.31.22",
    "@types/twilio": "0.0.10",
    "@types/xml2js": "^0.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "jasmine": "^3.5.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "mkdirp": "^0.5.1",
    "mongo-clone": "^1.2.0",
    "ncp": "^2.0.0",
    "node-flags": "^0.1.9",
    "node-ssh": "^5.1.2",
    "request-promise-native": "^1.0.7",
    "scss-compile": "^0.1.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.5",
    "xml2js-es6-promise": "^1.1.1"
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "app": "cross-env NODE_ENV=development node build/cloud/app",
    "build": "tsc --sourcemap",
    "build:prod": "tsc",
    "build:scss": "npx node-sass assets/templates/scss -o assets/templates/css",
    "clean": "rm -r -f build/cloud",
    "dashboard": "parse-dashboard --config config/parse-dashboard.config.json",
    "debug": "cross-env NODE_ENV=development node --inspect node_modules/parse-server/bin/parse-server config/parse-server.config.js",
    "lint": "tslint src/**/*.ts --project src/tsconfig.json",
    "mongodb:start": "net start MongoDB",
    "mongodb:stop": "net stop MongoDB",
    "mongodb-runner:start": "MONGODB_VERSION=4.0.4 mongodb-runner start",
    "mongodb-runner:stop": "mongodb-runner stop",
    "prebuild": "npm run clean",
    "server": "cross-env NODE_ENV=development npx parse-server config/parse-server.config.js",
    "test": "npx jasmine-ts"
  },
  "dependencies": {
    "ejs": "^2.6.2",
    "i18n-iso-countries": "^4.0.0",
    "module-alias": "^2.2.0",
    "node-gcm": "^1.0.2",
    "nodemailer": "^6.2.1",
    "parse-server-mailgun": "^2.4.15",
    "password-generator": "^2.2.3",
    "stripe": "^7.4.0",
    "styliner": "^1.0.4",
    "twilio": "^3.34.0"
  },
  "_moduleAliases": {
    "@app/env": "build/cloud/env/env",
    "@lib": "build/cloud/lib",
    "@modules": "build/cloud/modules",
    "parse": "build/cloud/parse.server"
  }
}
